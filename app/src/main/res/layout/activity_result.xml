                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/unit_of_rbc"
                        android:textColor="@color/black"
                        android:textSize="16sp" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/hgb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
